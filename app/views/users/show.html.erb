<div class="container">
  <div class="dashboard-header">
    <h3>Stats</h3>
    <div class="dashboard-header-content row">
      <%= render('dashboard_header') %>
    </div>
  </div>

  <div class="dashboard-content row">
    <div class="positions_pannel_container flex-container  col-12 col-md-6">
      <h3>Positions</h3>
      <div class="positions_pannel">
        <%= render('investment_cards') %>
      </div>
    </div>
    <div class="transactions_pannel_container flex-container  col-12 col-md-6">
      <h3>Latest transactions</h3>
      <%= render('transactions_pannel') %>
    </div>
  </div>
</div>
